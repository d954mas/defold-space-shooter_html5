[project]
title = Asteroid destroyer

[bootstrap]
main_collection = /main/main.collectionc
render = /render/default.renderc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 480
height = 640
variable_dt = 0

[physics]
scale = 0.02

[script]
shared_state = 1

[sprite]
max_count = 256

[android]
