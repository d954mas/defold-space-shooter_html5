app_icon_96x96 = /icons/android/mipmap-xhdpi/ic_launcher.png
app_icon_144x144 = /icons/android/mipmap-xxhdpi/ic_launcher.png
app_icon_192x192 = /icons/android/mipmap-xxxhdpi/ic_launcher.png
package = com.d954mas.game.asteroiddestroyer

